<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomButton">
        <attr name="customType"/>
        <attr name="android:gravity">center</attr>
    </declare-styleable>
    <attr name="customType">
        <enum name="flex" value="1"/>
        <enum name="fix" value="2"/>
    </attr>
    <attr name="myViewStyle" format="reference"/>
    <declare-styleable name="CustomProgressView">
        <attr name="label_start" format="string"/>
        <attr name="label_end" format="string"/>
        <attr name="label_center" format="string"/>
        <attr name="label_center_desc" format="string"/>
        <attr name="progress" format="integer"/>
        <attr name="progressColor" format="color"/>
        <attr name="progressTrackColor" format="color"/>
        <attr name="progressArcThickness" format="dimension"/>
        <attr name="progressTrackThickness" format="dimension"/>
        <attr name="label_start_textSize" format="dimension"/>
        <attr name="label_end_textSize" format="dimension"/>
        <attr name="label_center_textSize" format="dimension"/>
        <attr name="label_center_desc_textSize" format="dimension"/>
        <attr name="progressThumbSize" format="dimension"/>
    </declare-styleable>

</resources>